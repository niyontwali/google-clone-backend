{
  "name": "google-clone",
  "version": "1.0.0",
  "description": "Google Clone",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "rollback": "npx sequelize-cli db:migrate:undo",
    "rollback:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "rollback:seed": "npx sequelize-cli db:seed:undo",
    "rollback:seed:all": "npx sequelize-cli db:seed:undo:all",
    "format": "prettier \"**/*.{js,ts}\" --write"
  },
  "author": "John Niyontwali",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  }
}
